{
    "name": "webpack-i18n-static-export",
    "version": "1.0.0",
    "repository": "https://github.com/penhao/webpack-i18n-static-export.git",
    "author": "penhao <penhao817@gmail.com>",
    "license": "MIT",
    "scripts": {
        "i18n-watch": "watch 'npm run i18n' wwwroot/i18n",
        "i18n": "rimraf wwwroot/i18n && static-i18n -l en -i en -i zh wwwroot --output-dir wwwroot/i18n",
        "dev": "webpack serve --config webpack.dev.js --progress",
        "build": "npm run i18n && webpack --config webpack.prod.js"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.14.0",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^8.1.1",
        "css-loader": "^5.2.4",
        "file-loader": "^6.2.0",
        "glob": "^7.1.6",
        "html-webpack-plugin": "^5.3.1",
        "mini-css-extract-plugin": "^1.6.0",
        "node-sass": "^5.0.0",
        "normalize.css": "^8.0.1",
        "postcss": "^8.2.13",
        "postcss-loader": "^5.2.0",
        "postcss-preset-env": "^6.7.0",
        "resolve-url-loader": "^4.0.0-beta.2",
        "sass-loader": "^11.0.1",
        "static-i18n": "^0.2.10",
        "style-loader": "^2.0.0",
        "webpack": "^5.36.2",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3",
        "webpack-remove-empty-scripts": "^0.7.1",
        "webpack-static-i18n-plugin": "^1.0.1"
    },
    "dependencies": {
        "tailwindcss": "^2.1.2",
        "validator": "^13.6.0"
    }
}
